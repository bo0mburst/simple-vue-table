{
  "name": "simple-vue-table",
  "version": "1.0.3",
  "description": "A vue component that display tabular data",
  "license": "MIT",
  "keywords": [
    "vue table",
    "simple vue table",
    "simple vue component"
  ],
  "author": {
    "name": "Billy Sillano",
    "email": "billysillano@gmail.com"
  },
  "main": "dist/simple-vue-table.ssr.js",
  "module": "dist/simple-vue-table.esm.js",
  "unpkg": "dist/simple-vue-table.min.js",
  "files": [
    "dist/*",
    "src/**/*.vue"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "dependencies": {},
  "devDependencies": {
    "cross-env": "^5.2.0",
    "minimist": "^1.2.6",
    "rollup": "^1.32.0",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-vue": "^5.1.6",
    "vue": "^2.6.11",
    "vue-template-compiler": "^2.6.11"
  }
}
